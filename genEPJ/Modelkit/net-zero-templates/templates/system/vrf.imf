<%#INITIALIZE
parameter "system_name"
parameter "zone_names"

parameter "control_zone_name", :default=>""
parameter "heat_recovery", :default=>true
parameter "vrf_condenser", :default=>"AIRCOOLED"

parameter "manufacturer", :default=>"Mitsubishi"  # (Mitsubishi | Daikin)

parameter "heating_cop", :default=>3.5484
parameter "cooling_cop", :default=>3.2917

parameter "num_compressors", :default=>3

# There's a tenuous agreement between the system (this template) and the zone HVAC equipment
# that must agree on the convention for the terminal unit names!

parameter "operation_schedule", :default=>
"
  Through: 12/31,          !- Field 1
  For: AllDays,            !- Field 2
  Until: 24:00, 1.0;       !- Field 3
"

%>

AirConditioner:VariableRefrigerantFlow,
  <%= system_name %> VRF Heat Pump,           !- Heat Pump Name
  <%= system_name %> Operation Schedule,  !- Availability Schedule Name
  Autosize,                !- Rated Total Cooling Capacity {W}
  <%= cooling_cop %>,      !- Rated Cooling COP {W/W}
  -25,                      !- Minimum Outdoor Temperature in Cooling Mode {C}
  43,                      !- Maximum Outdoor Temperature in Cooling Mode {C}
  <%= system_name %> VRFCoolCapFT,            !- Cooling Capacity Ratio Modifier Function of Low Temperature Curve Name
  <%= system_name %> VRFCoolCapFTBoundary,    !- Cooling Capacity Ratio Boundary Curve Name
  <%= system_name %> VRFCoolCapFTHi,          !- Cooling Capacity Ratio Modifier Function of High Temperature Curve Name
  <%= system_name %> VRFCoolEIRFT,            !- Cooling Energy Input Ratio Modifier Function of Low Temperature Curve Name
  <%= system_name %> VRFCoolEIRFTBoundary,    !- Cooling Energy Input Ratio Boundary Curve Name
  <%= system_name %> VRFCoolEIRFTHi,          !- Cooling Energy Input Ratio Modifier Function of High Temperature Curve Name
  <%= system_name %> CoolingEIRLowPLR,        !- Cooling Energy Input Ratio Modifier Function of Low Part-Load Ratio Curve Name
  <%= system_name %> CoolingEIRHiPLR,         !- Cooling Energy Input Ratio Modifier Function of High Part-Load Ratio Curve Name
  <%= system_name %> CoolingCombRatio,        !- Cooling Combination Ratio Correction Factor Curve Name
  <%= system_name %> VRFCPLFFPLR,             !- Cooling Part-Load Fraction Correlation Curve Name
  Autosize,                !- Rated Total Heating Capacity {W}
  1.0,                     !- Rated Total Heating Capacity Sizing Ratio {W/W}
  <%= heating_cop %>,      !- Rated Heating COP {W/W}
  -20,                     !- Minimum Outdoor Temperature in Heating Mode {C}
  100,                      !- Maximum Outdoor Temperature in Heating Mode {C}
  <%= system_name %> VRFHeatCapFT,            !- Heating Capacity Ratio Modifier Function of Low Temperature Curve Name
  <%= system_name %> VRFHeatCapFTBoundary,    !- Heating Capacity Ratio Boundary Curve Name
  <%= system_name %> VRFHeatCapFTHi,          !- Heating Capacity Ratio Modifier Function of High Temperature Curve Name
  <%= system_name %> VRFHeatEIRFT,            !- Heating Energy Input Ratio Modifier Function of Low Temperature Curve Name
  <%= system_name %> VRFHeatEIRFTBoundary,    !- Heating Energy Input Ratio Boundary Curve Name
  <%= system_name %> VRFHeatEIRFTHi,          !- Heating Energy Input Ratio Modifier Function of High Temperature Curve Name
  WetBulbTemperature,      !- Heating Performance Curve Outdoor Temperature Type
  <%= system_name %> HeatingEIRLowPLR,        !- Heating Energy Input Ratio Modifier Function of Low Part-Load Ratio Curve Name
  <%= system_name %> HeatingEIRHiPLR,         !- Heating Energy Input Ratio Modifier Function of High Part-Load Ratio Curve Name
  <%= system_name %> HeatingCombRatio,        !- Heating Combination Ratio Correction Factor Curve Name
  <%= system_name %> VRFCPLFFPLR,             !- Heating Part-Load Fraction Correlation Curve Name
  0.25,                    !- Minimum Heat Pump Part-Load Ratio
  <%= control_zone_name %>,  !- Zone Name for Master Thermostat Location
  LoadPriority,            !- Master Thermostat Priority Control Type
  ,                        !- Thermostat Priority Schedule Name
  <%= system_name %> VRF Terminal Unit List,  !- Zone Terminal Unit List Name
  <% if (heat_recovery) %>
  Yes,                     !- Heat Pump Waste Heat Recovery
  <% else %>
  No,                      !- Heat Pump Waste Heat Recovery
  <% end %>
  30,                      !- Equivalent Piping Length used for Piping Correction Factor in Cooling Mode {m}
  10,                      !- Vertical Height used for Piping Correction Factor {m}
  <%= system_name %> CoolingLengthCorrectionFactor,  !- Piping Correction Factor for Length in Cooling Mode Curve Name
  -0.000386,               !- Piping Correction Factor for Height in Cooling Mode Coefficient
  30,                      !- Equivalent Piping Length used for Piping Correction Factor in Heating Mode {m}
  ,                        !- Piping Correction Factor for Length in Heating Mode Curve Name
  ,                        !- Piping Correction Factor for Height in Heating Mode Coefficient
  15,                      !- Crankcase Heater Power per Compressor {W}
  <%= num_compressors %>,  !- Number of Compressors
  ,                        !- Ratio of Compressor Size to Total Compressor Capacity
  7,                       !- Maximum Outdoor Dry-bulb Temperature for Crankcase Heater {C}
  Resistive,               !- Defrost Strategy
  Timed,                   !- Defrost Control
  ,                        !- Defrost Energy Input Ratio Modifier Function of Temperature Curve Name
  ,                        !- Defrost Time Period Fraction
  0.0000001,               !- Resistive Defrost Heater Capacity {W}
  7,                       !- Maximum Outdoor Dry-bulb Temperature for Defrost Operation {C}
  <%= vrf_condenser %>,    !- Condenser Type
<% if (vrf_condenser == "WATERCOOLED") %>
  <%= system_name %> VRF Heat Pump Water Inlet Node,  !- Condenser Inlet Node Name
  <%= system_name %> VRF Heat Pump Water Outlet Node,  !- Condenser Outlet Node Name
  Autosize,                !- Water Condenser Volume Flow Rate
<% else %>
  <%= system_name %> VRF Outdoor Air Node,  !- Condenser Inlet Node Name
  ,                        !- Condenser Outlet Node Name
  ,                        !- Water Condenser Volume Flow Rate
<% end %>
  ,                        !- Evaporative Condenser Effectiveness {dimensionless}
  ,                        !- Evaporative Condenser Air Flow Rate {m3/s}
  0,                       !- Evaporative Condenser Pump Rated Power Consumption {W}
  ,                        !- Supply Water Storage Tank Name
  0,                       !- Basin Heater Capacity {W/K}
  ,                        !- Basin Heater Setpoint Temperature {C}
  ,                        !- Basin Heater Operating Schedule Name
  Electricity,             !- Fuel Type
  <% if (heat_recovery) # Use all defaults %>
  ,                        !- Minimum Outdoor Temperature in Heat Recovery Mode {C}
  ,                        !- Maximum Outdoor Temperature in Heat Recovery Mode {C}
  ,                        !- Heat Recovery Cooling Capacity Modifier Curve Name
  ,                        !- Initial Heat Recovery Cooling Capacity Fraction
  ,                        !- Heat Recovery Cooling Capacity Time Constant {hr}
  ,                        !- Heat Recovery Cooling Energy Modifier Curve Name
  ,                        !- Initial Heat Recovery Cooling Energy Fraction
  ,                        !- Heat Recovery Cooling Energy Time Constant {hr}
  ,                        !- Heat Recovery Heating Capacity Modifier Curve Name
  ,                        !- Initial Heat Recovery Heating Capacity Fraction
  ,                        !- Heat Recovery Heating Capacity Time Constant {hr}
  ,                        !- Heat Recovery Heating Energy Modifier Curve Name
  ,                        !- Initial Heat Recovery Heating Energy Fraction
  ;                        !- Heat Recovery Heating Energy Time Constant {hr}
  <% else %>
  ,                        !- Minimum Outdoor Temperature in Heat Recovery Mode {C}
  ,                        !- Maximum Outdoor Temperature in Heat Recovery Mode {C}
  ,                        !- Heat Recovery Cooling Capacity Modifier Curve Name
  ,                        !- Initial Heat Recovery Cooling Capacity Fraction
  0.15,                    !- Heat Recovery Cooling Capacity Time Constant {hr}
  ,                        !- Heat Recovery Cooling Energy Modifier Curve Name
  1.0,                     !- Initial Heat Recovery Cooling Energy Fraction
  0.0,                     !- Heat Recovery Cooling Energy Time Constant {hr}
  ,                        !- Heat Recovery Heating Capacity Modifier Curve Name
  1.0,                     !- Initial Heat Recovery Heating Capacity Fraction
  0.15,                    !- Heat Recovery Heating Capacity Time Constant {hr}
  ,                        !- Heat Recovery Heating Energy Modifier Curve Name
  1.0,                     !- Initial Heat Recovery Heating Energy Fraction
  0.0;                     !- Heat Recovery Heating Energy Time Constant {hr}
  <% end %>

Schedule:Compact,
  <%= system_name %> Operation Schedule,  !- Name
  On/Off,                  !- Schedule Type Limits Name
<%= operation_schedule %>

ZoneTerminalUnitList,
  <%= system_name %> VRF Terminal Unit List,  !- Zone Terminal Unit List Name
<% Modelkit::EnergyPlus.each(zone_names) do |zone_name| %>
  <%= zone_name %> VRF Terminal Unit,  !- Zone Terminal Unit Name
<% end %>

<% if (vrf_condenser == "WATERCOOLED") %>
Branch,
  <%= system_name %> HR Demand Branch,  !- Name
  ,                        !- Pressure Drop Curve Name
  AirConditioner:VariableRefrigerantFlow,  !- Component 1 Object Type
  <%= system_name %> VRF Heat Pump,   !- Component 1 Name
  <%= system_name %> VRF Heat Pump Water Inlet Node,  !- Component 1 Inlet Node Name
  <%= system_name %> VRF Heat Pump Water Outlet Node;  !- Component 1 Outlet Node Name
<% else %>
OutdoorAir:Node,
  <%= system_name %> VRF Outdoor Air Node;  !- Name
<% end %>



